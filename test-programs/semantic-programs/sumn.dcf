class Program {
  int sumn(int n) {
    return (n*(n+1))/2;
  }
  int getInt() {
    int ret, input;
    ret = 0;
    for thisLanguageHasNoWhileLoop = 1, 2 {
      thisLanguageHasNoWhileLoop -= 1;

      input = callout("getchar");
      if (input >= 48 && input <= 57) {
        ret = 10*ret + input - 48;
