int main(int x){
	// Assignment
	x = 1005;
	printf("x:%d\n",x);

	// BODMAS
	int d;
	d = 5*3 + 5;
	printf("d:%d\n",d);

	// Declaration + Defination
	int f = 3;
	printf("f:%d\n",f);

	// For Loop
	for (int i = 0; i < x; i+=1)
		printf("%d",i);
	;
	printf("\n");

	// While Loop
	while( x < 5){
		x -=1;
		printf("%d",x);
		break;
	};
	printf("\n");

	return 0;
};